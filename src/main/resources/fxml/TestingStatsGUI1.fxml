<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>

<BorderPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="app.ui.gui.TestingStatsGUI1">
   <top>
       <MenuBar nodeOrientation="RIGHT_TO_LEFT" BorderPane.alignment="CENTER">
           <menus>
               <Menu mnemonicParsing="false" text="Options">
                   <items>
                       <MenuItem mnemonicParsing="false" onAction="#menu_cancel" text="Cancel" />
                   </items></Menu>
           </menus>
       </MenuBar>
   </top>
   <center>
      <Pane prefHeight="213.0" prefWidth="357.0" BorderPane.alignment="CENTER">
         <children>
            <Button fx:id="btnTestsWaitingForResult" layoutX="170.0" layoutY="111.0" mnemonicParsing="false" onAction="#testsWaitingForResult" text="Tests Waiting For Diagnosis">
               <font>
                  <Font size="18.0" />
               </font>
            </Button>
            <Button fx:id="btnTestsWaitingForDiagnosis" layoutX="180.0" layoutY="45.0" mnemonicParsing="false" onAction="#testsWaitingForDiagnosis" text="Tests Waiting For Results">
               <font>
                  <Font size="18.0" />
               </font>
            </Button>
            <Button fx:id="btnTestsValidated" layoutX="219.0" layoutY="172.0" mnemonicParsing="false" onAction="#validatedTestsGraphs" text="Tests Validated">
               <font>
                  <Font size="18.0" />
               </font>
            </Button>
            <Label layoutX="133.0" layoutY="261.0" text="Total Client Count:">
               <font>
                  <Font name="System Bold" size="18.0" />
               </font>
            </Label>
            <Label layoutX="109.0" layoutY="293.0" text="Total Validated Tests:">
               <font>
                  <Font name="System Bold" size="18.0" />
               </font>
            </Label>
            <Label fx:id="lblTotalClient" layoutX="300.0" layoutY="264.0" prefHeight="21.0" prefWidth="54.0" />
            <Label fx:id="lblTotalValidatedTests" layoutX="301.0" layoutY="296.0" prefHeight="21.0" prefWidth="53.0" />
         </children>
      </Pane>
   </center>
</BorderPane>
